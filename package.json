{
  "publisher": "antfu",
  "name": "cpupro",
  "displayName": "CPUpro",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/cpupro#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/cpupro"
  },
  "bugs": {
    "url": "https://github.com/antfu/cpupro/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.89.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "cpupro.editor",
        "displayName": "CPUpro",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.cpuprofile"
          }
        ]
      }
    ],
    "commands": [
    ],
    "configuration": [
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^20.12.12",
    "@types/vscode": "^1.89.0",
    "@vscode/vsce": "^2.26.1",
    "bumpp": "^9.4.1",
    "cpupro": "^0.5.1",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  }
}
