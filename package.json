{
  "publisher": "antfu",
  "name": "cpupro",
  "displayName": "CPUpro",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "description": "CPUpro for opening .cpuprofile files in VS Code.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vscode-cpupro#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-cpupro"
  },
  "bugs": {
    "url": "https://github.com/antfu/vscode-cpupro/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "cpupro.editor",
        "displayName": "CPUpro",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.cpuprofile"
          }
        ]
      }
    ],
    "commands": [],
    "configuration": []
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@antfu/ni": "^24.4.0",
    "@types/node": "^22.15.19",
    "@types/vscode": "^1.100.0",
    "@vscode/vsce": "^3.4.1",
    "bumpp": "^10.1.1",
    "cpupro": "^0.5.1",
    "eslint": "^9.27.0",
    "esno": "^4.8.0",
    "pnpm": "^10.11.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
